{
  "version": 1,
  "projects": {
    "text-break": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/text-break",
      "sourceRoot": "apps/text-break/src",
      "prefix": "flow-design",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/text-break",
            "index": "apps/text-break/src/index.html",
            "main": "apps/text-break/src/main.ts",
            "polyfills": "apps/text-break/src/polyfills.ts",
            "tsConfig": "apps/text-break/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/text-break/src/favicon.ico",
              "apps/text-break/src/assets"
            ],
            "styles": [
              "apps/text-break/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/text-break/src/environments/environment.ts",
                  "with": "apps/text-break/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "text-break:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "text-break:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "text-break:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/text-break/tsconfig.app.json",
              "apps/text-break/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/text-break/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/text-break/jest.config.js",
            "tsConfig": "apps/text-break/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/text-break/src/test-setup.ts"
          }
        }
      }
    },
    "text-break-e2e": {
      "root": "apps/text-break-e2e",
      "sourceRoot": "apps/text-break-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/text-break-e2e/cypress.json",
            "tsConfig": "apps/text-break-e2e/tsconfig.e2e.json",
            "devServerTarget": "text-break:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "text-break:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/text-break-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/text-break-e2e/**"
            ]
          }
        }
      }
    },
    "text-wrap": {
      "root": "libs/text-wrap",
      "sourceRoot": "libs/text-wrap/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/text-wrap/tsconfig.lib.json",
              "libs/text-wrap/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/text-wrap/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/text-wrap/jest.config.js",
            "tsConfig": "libs/text-wrap/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "text-break-cli": {
      "root": "apps/text-break-cli",
      "sourceRoot": "apps/text-break-cli/src",
      "projectType": "application",
      "prefix": "text-break-cli",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/text-break-cli",
            "main": "apps/text-break-cli/src/main.ts",
            "tsConfig": "apps/text-break-cli/tsconfig.app.json",
            "assets": [
              "apps/text-break-cli/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/text-break-cli/src/environments/environment.ts",
                  "with": "apps/text-break-cli/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "text-break-cli:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/text-break-cli/tsconfig.app.json",
              "apps/text-break-cli/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/text-break-cli/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/text-break-cli/jest.config.js",
            "tsConfig": "apps/text-break-cli/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "text-break"
}
